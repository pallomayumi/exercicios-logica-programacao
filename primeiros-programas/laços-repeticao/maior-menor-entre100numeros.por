programa {
  funcao inicio() {
    inteiro valor,maior,menor,valor2
    valor=0

    escreva("informe o primeiro valor:")
    leia(valor)
    maior=valor
    menor=valor
    para(inteiro contagem=1;contagem<100;contagem++){
      leia(valor2)
    }
    se(valor2>maior){
      maior=valor2
    }
    se(valor2<menor){
      menor=valor2
    }

    
  }
}
